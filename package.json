{
  "name": "@honestica/ts-predicate",
  "version": "0.0.0-dev",
  "description": "TypeScript predicate and typeguard library",
  "author": "Benjamin Blum <vuk.kulvar+github@gmail.com>",
  "contributors": [
    "Lifen SAS <ops@lifen.fr>"
  ],
  "homepage": "https://github.com/honestica/typeguard#readme",
  "bugs": "https://github.com/honestica/typeguard/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/honestica/typeguard.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": " https://registry.npmjs.org"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    }
  },
  "files": [
    "/lib/**/*"
  ],
  "types": "./lib/types/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -b tsconfig.build.cjs.json",
    "build:esm": "tsc -b tsconfig.build.esm.json",
    "clean": "rm -rfv reports coverage build dist lib .eslintcache",
    "cspell:check": "cspell '**' '.**/**' '.*' --show-suggestions --config '.vscode/cspell.json'",
    "cspell:ci": "cspell lint '**' '.**/**' '.*' --show-suggestions --config '.vscode/cspell.json'",
    "eslint:ci": "eslint . --ext js,ts,cjs,mjs --format json -o reports/eslint/honestica-ts-predicate-eslint-junit.json",
    "eslint:fix": "eslint . --ext js,ts,cjs,mjs --fix",
    "postinstall": "husky install",
    "markdownlint:lint": "markdownlint-cli2 '**/*.md'",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "packagejson:format": "prettier-package-json --write ./package.json",
    "packagejson:lint": "npmPkgJsonLint .",
    "test": "yarn test:unit",
    "test:mutation": "npx stryker run",
    "test:unit": "mocha",
    "ts:check": "tsc --noEmit",
    "prettier:ci": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "devDependencies": {
    "@stryker-mutator/core": "^6.0.2",
    "@stryker-mutator/mocha-runner": "^6.0.2",
    "@stryker-mutator/typescript-checker": "^6.0.2",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "chai": "^4.3.6",
    "cspell": "^6.1.2",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-sort-exports": "^0.6.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.1",
    "markdownlint-cli2": "^0.4.0",
    "mocha": "^10.0.0",
    "npm-package-json-lint": "^6.3.0",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2",
    "prettier-package-json": "^2.6.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "packageManager": "yarn@3.2.1",
  "keywords": [
    "guard",
    "honestica",
    "lifen",
    "predicate",
    "ts",
    "ts-predicate",
    "type",
    "type-guard",
    "typeguard",
    "typescript"
  ]
}
